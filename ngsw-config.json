{
  "index": "/index.html",
  "external": {
    "urls": [
      { "url": "https://fonts.googleapis.com/css?family=Dosis:400"},
      { "url": "https://fonts.googleapis.com/css?family=Quicksand:400,700"}
    ]
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ]
}
